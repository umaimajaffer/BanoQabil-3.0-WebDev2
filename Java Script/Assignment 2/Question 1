
let a = 10;
console.log("Result:")
console.log("The value of a is: ",a)
console.log()

console.log("The value of ++a is:" ,++a)
console.log("The value of a is: ",a)
console .log()

console.log("The value of a++ is:" ,a++)
console.log("The value of a is: ",a)
console .log()

console.log("The value of --a is:" ,--a)
console.log("The value of a is: ",a)
console .log()

console.log("The value of a-- is:" ,a--)
console.log("The value of a is: ",a)
console .log()
